{
  "name": "board-game-planner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm run codegen && next build",
    "build-storybook": "build-storybook",
    "codegen": "graphql-let",
    "coverage": "npm test -- --coverage",
    "coverage:watch": "npm run coverage -- --watch",
    "dev": "npm run codegen && next dev",
    "docker:down": "npm run docker:kill && npm run docker:rm",
    "docker:kill": "docker-compose -f docker-compose.dev.yaml kill",
    "docker:mysql": "docker exec -it `docker ps | grep mysql | awk '{print $1}'` bash",
    "docker:pull": "docker-compose -f docker-compose.dev.yaml pull",
    "docker:rm": "docker-compose -f docker-compose.dev.yaml rm -f",
    "docker:up": "docker-compose -f docker-compose.dev.yaml up -d",
    "entity:create": "npm run typeorm entity:create",
    "lint": "eslint . --ext .ts,.tsx",
    "migration:create": "npm run typeorm migration:create",
    "migration:run": "npm run typeorm migration:run",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@apollo/client": "^3.2.9",
    "@graphql-tools/load-files": "^6.2.5",
    "@graphql-tools/merge": "^6.2.5",
    "@graphql-tools/schema": "^7.1.0",
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "apollo-server-micro": "^2.19.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.12.2",
    "fast-xml-parser": "^3.17.4",
    "fontsource-roboto": "^3.0.3",
    "graphql": "^15.4.0",
    "he": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5",
    "next": "10.0.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.2.22"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@graphql-codegen/cli": "^1.19.3",
    "@graphql-codegen/plugin-helpers": "^1.18.2",
    "@graphql-codegen/typescript": "^1.18.1",
    "@graphql-codegen/typescript-operations": "^1.17.12",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@graphql-codegen/typescript-resolvers": "^1.17.12",
    "@storybook/addon-actions": "^6.1.10",
    "@storybook/addon-essentials": "^6.1.10",
    "@storybook/addon-links": "^6.1.10",
    "@storybook/addon-storyshots": "^6.1.10",
    "@storybook/react": "^6.1.10",
    "@types/bcryptjs": "^2.4.2",
    "@types/he": "^1.1.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-test-renderer": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel-loader": "^8.2.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fetch-mock": "^9.11.0",
    "graphql-let": "^0.16.3",
    "graphql-tag": "^2.11.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "react-test-renderer": "^17.0.1",
    "typescript": "^4.1.2",
    "yaml-loader": "^0.6.0"
  }
}
